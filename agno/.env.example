# Agno Skill Runner Configuration
# Copy this file to .env and fill in your keys

GOOGLE_API_KEY=your_google_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Vertex AI Configuration
# Set USE_VERTEX_AI=true and provide your GCP project ID to use Google Cloud Vertex AI instead of Google AI Studio.
# You will also need to ensure your environment is authenticated (e.g. gcloud auth application-default login)
USE_VERTEX_AI=false
GOOGLE_CLOUD_PROJECT=your_gcp_project_id
